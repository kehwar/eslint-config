{
    "name": "@kehwar/eslint-config",
    "type": "module",
    "version": "1.0.11",
    "license": "MIT",
    "homepage": "https://github.com/kehwar/eslint-config",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "typesVersions": {
        "*": {
            "*": [
                "./dist/*",
                "./dist/index.d.ts"
            ]
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "dev": "unbuild --stub",
        "lint": "eslint .",
        "lint-staged": "eslint --fix",
        "typecheck": "tsc --noEmit",
        "release": "pnpm run build && release-it --ci"
    },
    "peerDependencies": {
        "eslint": "^8.53.0"
    },
    "dependencies": {
        "@antfu/eslint-config": "1.1.2",
        "@eslint/eslintrc": "^2.1.3",
        "eslint-plugin-lodash": "^7.4.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-sonarjs": "^0.23.0",
        "eslint-plugin-tailwindcss": "^3.13.0"
    },
    "devDependencies": {
        "@types/eslint": "^8.44.7",
        "@types/node": "^20.9.1",
        "eslint": "^8.53.0",
        "release-it": "^17.0.0",
        "typescript": "^5.2.2",
        "unbuild": "^2.0.0"
    }
}
